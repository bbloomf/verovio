<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="4.0.0">
	<meiHead>
		<fileDesc>
			<titleStmt>
				<title>BracketSpan example</title>
			</titleStmt>
			<pubStmt>
				<date>2018-12-19</date>
			</pubStmt>
			<notesStmt>
				<annot>Verovio supports bracketSpan for representing ligatures or coloration in CMN.</annot>
			</notesStmt>
		</fileDesc>
		<encodingDesc>
			<appInfo>
				<application version="unknown">
					<name>Verovio</name>
				</application>
			</appInfo>
		</encodingDesc>
	</meiHead>
	<music>
		<body>
			<mdiv>
				<score>
					<scoreDef>
						<staffGrp>
							<staffDef n="1" xml:id="s1" clef.line="2" clef.shape="G" lines="5"/>
						</staffGrp>
					</scoreDef>
					<section>
						<measure label="feature-example" n="1">
							<staff n="1">
								<layer n="1">
									<note dur="1" pname="c" oct="4" xml:id="n01"/>
									<note dur="1" pname="d" oct="4" xml:id="n02"/>
									<note dur="1" pname="e" oct="4" xml:id="n03"/>
									<note dur="1" pname="f" oct="4" xml:id="n04"/>
									<note dur="1" pname="g" oct="4" xml:id="n05"/>
									<note dur="1" pname="f" oct="4" xml:id="n06"/>
									<note dur="1" pname="e" oct="4" xml:id="n07"/>
									<note dur="1" pname="d" oct="4" xml:id="n08"/>
									<note dur="1" pname="c" oct="4" xml:id="n09"/>
									<note dur="1" pname="g" oct="4" xml:id="n10"/>
								</layer>
							</staff>
							<bracketSpan func="ligature" startid="#n01" endid="#n02" staff="1"/>
							<bracketSpan func="ligature" startid="#n03" endid="#n04" staff="1" lwidth="medium"/>
							<bracketSpan func="ligature" startid="#n05" endid="#n06" staff="1" lwidth="0.5vu"/>
							<bracketSpan func="ligature" startid="#n07" endid="#n08" staff="1" lform="dotted"/>
							<bracketSpan func="ligature" startid="#n09" endid="#n10" staff="1" lform="solid"/>
							<bracketSpan func="ligature" tstamp="1.0" tstamp2="0m+41.0" staff="1" lform="dashed"/>
						</measure>
					</section>
				</score>
			</mdiv>
		</body>
	</music>
</mei>
