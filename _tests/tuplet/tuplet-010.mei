<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="4.0.0">
	<meiHead>
		<fileDesc>
			<titleStmt>
				<title>Nested tuplets</title>
			</titleStmt>
			<pubStmt>
				<date>2017-04-24</date>
			</pubStmt>
			<notesStmt>
				<annot>Verovio appropriately separates numbers and brackets belonging to different, nested tuplets.</annot>
			</notesStmt>
		</fileDesc>
		<encodingDesc>
			<appInfo>
				<application version="unknown">
					<name>Verovio</name>
				</application>
			</appInfo>
		</encodingDesc>
	</meiHead>
	<music>
		<body>
			<mdiv>
				<score>
					<scoreDef meter.count="4" meter.unit="4">
						<staffGrp>
							<staffDef n="1" clef.line="1" clef.shape="C" lines="5"/>
						</staffGrp>
					</scoreDef>
					<section>
						<measure>
							<staff n="1">
								<layer n="1">
									<tuplet num="7" numbase="8">
										<note pname="c" oct="5" dur="8"/>
										<note pname="c" oct="4" dur="8"/>
										<beam>
											<tuplet num="5" numbase="4" num.format="ratio">
												<tuplet num="3" numbase="2">
													<note pname="e" oct="4" dur="8"/>
													<note pname="f" oct="4" dur="8"/>
													<note pname="g" oct="4" dur="8"/>
												</tuplet>
												<beam>
													<tuplet num="3" numbase="2">
														<note pname="b" oct="4" dur="8"/>
														<note pname="e" oct="4" dur="8"/>
														<note pname="b" oct="3" dur="8"/>
													</tuplet>
												</beam>
												<note pname="c" oct="4" dur="8"/>
											</tuplet>
										</beam>
										<note pname="a" oct="4" dur="8"/>
									</tuplet>
								</layer>
							</staff>
						</measure>
					</section>
				</score>
			</mdiv>
		</body>
	</music>
</mei>
