<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="4.0.0">
	<meiHead>
		<fileDesc>
			<titleStmt>
				<title>Score and barlines example</title>
			</titleStmt>
			<pubStmt>
				<date>2017-05-09</date>
			</pubStmt>
			<notesStmt>
				<annot>Verovio supports the score parameters defined in the "scoreDef", "staffGrp" and "staffDef" elements with some of their
					attributes. Verovio also supports various types of barline for the "measure" elements. It also supports the "scale" attribute on
					the "staffDef" element.</annot>
			</notesStmt>
		</fileDesc>
		<encodingDesc>
			<appInfo>
				<application version="1.0.0" label="2">
					<name>Verovio</name>
				</application>
			</appInfo>
		</encodingDesc>
	</meiHead>
	<music>
		<body>
			<mdiv>
				<score>
					<scoreDef label="feature-example">
						<staffGrp>
							<staffGrp symbol="bracket" bar.thru="true">
								<staffDef n="1" clef.line="2" clef.shape="G" lines="5" scale="66%"/>
								<staffGrp symbol="brace">
									<staffDef n="2" clef.line="2" clef.shape="G" lines="5" scale="66%"/>
									<staffDef n="3" clef.line="2" clef.shape="G" lines="5" scale="66%"/>
								</staffGrp>
							</staffGrp>
							<staffDef n="4" clef.line="2" clef.shape="G" lines="5"/>
							<staffGrp symbol="brace" bar.thru="false">
								<staffDef n="5" clef.line="2" clef.shape="G" lines="5"/>
								<staffDef n="6" clef.line="4" clef.shape="F" lines="5"/>
							</staffGrp>
						</staffGrp>
					</scoreDef>
					<section>
						<measure n="1">
							<staff n="1">
								<layer n="1"> </layer>
							</staff>
							<staff n="2">
								<layer n="1"> </layer>
							</staff>
							<staff n="3">
								<layer n="1"> </layer>
							</staff>
							<staff n="4">
								<layer n="1"> </layer>
							</staff>
							<staff n="5">
								<layer n="1"> </layer>
							</staff>
							<staff n="6">
								<layer n="1"> </layer>
							</staff>
						</measure>
						<measure n="2">
							<staff n="1">
								<layer n="1"> </layer>
							</staff>
							<staff n="2">
								<layer n="1"> </layer>
							</staff>
							<staff n="3">
								<layer n="1"> </layer>
							</staff>
							<staff n="4">
								<layer n="1"> </layer>
							</staff>
							<staff n="5">
								<layer n="1"> </layer>
							</staff>
							<staff n="6">
								<layer n="1"> </layer>
							</staff>
						</measure>
						<measure n="3" right="dbl">
							<staff n="1">
								<layer n="1"> </layer>
							</staff>
							<staff n="2">
								<layer n="1"> </layer>
							</staff>
							<staff n="3">
								<layer n="1"> </layer>
							</staff>
							<staff n="4">
								<layer n="1"> </layer>
							</staff>
							<staff n="5">
								<layer n="1"> </layer>
							</staff>
							<staff n="6">
								<layer n="1"> </layer>
							</staff>
						</measure>
						<measure n="4" right="rptstart">
							<staff n="1">
								<layer n="1"> </layer>
							</staff>
							<staff n="2">
								<layer n="1"> </layer>
							</staff>
							<staff n="3">
								<layer n="1"> </layer>
							</staff>
							<staff n="4">
								<layer n="1"> </layer>
							</staff>
							<staff n="5">
								<layer n="1"> </layer>
							</staff>
							<staff n="6">
								<layer n="1"> </layer>
							</staff>
						</measure>
						<measure n="5" right="rptboth">
							<staff n="1">
								<layer n="1"> </layer>
							</staff>
							<staff n="2">
								<layer n="1"> </layer>
							</staff>
							<staff n="3">
								<layer n="1"> </layer>
							</staff>
							<staff n="4">
								<layer n="1"> </layer>
							</staff>
							<staff n="5">
								<layer n="1"> </layer>
							</staff>
							<staff n="6">
								<layer n="1"> </layer>
							</staff>
						</measure>
						<measure n="6" right="rptend">
							<staff n="1">
								<layer n="1"> </layer>
							</staff>
							<staff n="2">
								<layer n="1"> </layer>
							</staff>
							<staff n="3">
								<layer n="1"> </layer>
							</staff>
							<staff n="4">
								<layer n="1"> </layer>
							</staff>
							<staff n="5">
								<layer n="1"> </layer>
							</staff>
							<staff n="6">
								<layer n="1"> </layer>
							</staff>
						</measure>
					</section>
				</score>
			</mdiv>
		</body>
	</music>
</mei>
