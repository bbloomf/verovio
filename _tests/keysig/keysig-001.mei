<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="4.0.0">
	<meiHead>
		<fileDesc>
			<titleStmt>
				<title>Key changes example</title>
			</titleStmt>
			<pubStmt>
				<date>2017-05-09</date>
			</pubStmt>
			<notesStmt>
				<annot>Verovio takes into account the "key.sig.showchange" and "key.sig.show" attributes which are assumed to be respectively false
					and true if not specified. It also shows cautionary key and time signatures at the end of a system when necessary.</annot>
			</notesStmt>
		</fileDesc>
		<encodingDesc>
			<appInfo>
				<application version="unkdown">
					<name>Verovio</name>
				</application>
			</appInfo>
		</encodingDesc>
	</meiHead>
	<music>
		<body>
			<mdiv>
				<score xml:id="score-0000000095007795">
					<scoreDef xml:id="scoredef-0000000517127186" midi.bpm="400" key.sig="5f" keysig.showchange="false" key.mode="aeolian">
						<staffGrp xml:id="staffgrp-0000000456171729" bar.thru="false">
							<staffDef xml:id="staffdef-0000000821767083" meter.count="4" meter.unit="2" n="1" lines="5">
								<label xml:id="label-0000000995964205">Canto</label>
								<labelAbbr xml:id="labelAbbr-0000001167810559">C</labelAbbr>
								<clef line="2" shape="G" visible="false"/>
								<keySig>
									<keyAccid pname="b" accid="1qf"/>
									<keyAccid pname="f" accid="s"/>
								</keySig>
							</staffDef>
						</staffGrp>
					</scoreDef>
					<section xml:id="section-L17F1">
						<measure xml:id="measure-L1" type="m--1">
							<staff xml:id="staff-0000001095140514" n="1">
								<layer xml:id="layer-L17F9N1" n="1">
									<rest xml:id="rest-L29F9" dur="2"/>
								</layer>
							</staff>
						</measure>
					</section>
				</score>
			</mdiv>
		</body>
	</music>
