<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="4.0.0">
  <meiHead>
        <fileDesc>
            <titleStmt>
                <title>Reverse mensural signs</title>
            </titleStmt>
            <pubStmt>
                <date>2018-07-03</date>
            </pubStmt>
            <notesStmt>
                <annot>Verovio supports reversed mensural signs.</annot>
            </notesStmt>
        </fileDesc>
        <encodingDesc>
            <appInfo>
                <application version="1.1">
                    <name>Verovio</name>
                </application>
            </appInfo>
        </encodingDesc>
  </meiHead>
  <music>
    <body>
      <mdiv>
        <score>
          <scoreDef>
            <staffGrp>
              <staffDef clef.shape="C" clef.line="1" mensur.slash="1" mensur.sign="C" mensur.orient="reversed" n="1" notationtype="mensural" lines="5">
