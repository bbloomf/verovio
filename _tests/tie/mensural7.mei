                <note dur="semibrevis" oct="4" pname="g" />
                <note dur="minima" oct="4" pname="f" />
                <note dur="minima" oct="4" pname="c" />
                <note dur="minima" oct="4" pname="f" />
                <dot />
                <note dur="semiminima" oct="4" pname="g" />
                <note dur="minima" oct="4" pname="a" />
              </layer>
            </staff>
          </section>
        </score>
