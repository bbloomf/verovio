<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://music-encoding.org/schema/4.0.0/mei-all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="4.0.0">
	<meiHead>
		<fileDesc>
			<titleStmt>
				<title>Example use of @sameas on note and clef</title>
			</titleStmt>
			<pubStmt>
				<date>2017-05-15</date>
			</pubStmt>
			<notesStmt>
				<annot>Note and clef can refer to another element using @sameas.</annot>
			</notesStmt>
		</fileDesc>
		<encodingDesc>
			<appInfo>
				<application version="unknown">
					<name>Verovio</name>
				</application>
			</appInfo>
		</encodingDesc>
	</meiHead>
	<music>
		<body>
			<mdiv>
				<score>
					<scoreDef xml:id="scoredef-0000000069132353" midi.bpm="120">
						<staffGrp xml:id="staffgrp-0000000875123578" bar.thru="true">
							<staffDef xml:id="staffdef-0000000714574911" n="1" clef.shape="G" clef.line="2" key.sig="2s" lines="5"/>
						</staffGrp>
					</scoreDef>
					<section xml:id="section-0000000734664354">
						<measure>
							<staff n="1">
								<layer n="1">
									<note dur="4" pname="c" oct="6" xml:id="n1"/>
									<clef line="4" shape="F" xml:id="c1"/>
									<note dur="4" pname="c" oct="3" xml:id="n2"/>
								</layer>
								<layer n="2">
									<note sameas="#n1"/>
									<clef sameas="#c1"/>
									<note dur="4" pname="c" oct="3" xml:id="n3"/>
								</layer>
							</staff>
						</measure>
					</section>
				</score>
			</mdiv>
		</body>
	</music>
</mei>
